{
  "commands": {
    "setup": {
      "name": "setup",
      "description": "Setup authentication channel on server",
      "options": [
        {
          "name": "role",
          "description": "Set a role to give users when they get verified"
        },
        {
          "name": "log",
          "description": "Set a log for users usage"
        },
        {
          "name": "language",
          "description": "Set a display language"
        }
      ]
    }
  },
  "panel": {
    "components": ["Card ID"]
  },
  "card_id": {
    "title": "Card ID : %{guild_name}",
    "fields": [
      {
        "name": "Details",
        "value": "・**Name** : %{user_name}\n・**ID** : %{user_id}\n・**Email** : %{user_email}"
      },
      {
        "name": "Account by XYOS",
        "value": "・**Updated** : <t:%{updated}:R>\n・**Verified** : <t:%{verified}:f>"
      }
    ]
  },
  "log": {
    "fields": [
      {
        "name": "Action : %{action}",
        "value": "・**User** : <@%{user_id}>\n・**ID** : %{user_id}"
      },
      {
        "name": "Timestamp",
        "value": "・**Date** : <t:%{unix}:f>\n・**Unix** : %{unix}"
      }
    ],
    "footer": "Logger : Authentication・Latency : %{latency} second(s)"
  },
  "common": {
    "wait": "<:utility_3:1355396255442010143> **Please wait, your request is being processed...**",
    "success": "<:utility_1:1355396027179602101> **Operation completed successfully**",
    "error": "<:utility_2:1355396240061497475> **Operation failed**"
  }
}
